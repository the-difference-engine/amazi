  <% if current_user %>
	  <div id="feedback_form">
		  <%= form_tag("/feedback", method: "post") do %>
		    <%= label_tag(:message, "Enter your message here:") %>
		    <%= text_field_tag(:message) %>
		    <%= submit_tag ("Send") %>
		  <% end %>
	  </div>
  <% else %>
  <h2> You must be logged in to send us feedback! </h2>
<% end %>